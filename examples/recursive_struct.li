#import "linux/print.li"

List :: struct {
    elem : s32;
    next : ^List;
}

print_list :: (root : ^List) {
    aux := root;
    for aux := root; aux != [^List]0; aux = (*aux).next {
        print_s32((*aux).elem);
        print_string("\n");
    }
}

main :: () -> s32{
    l1 : List;
    l2 : List;
    l3 : List;

    l1.elem = 1;
    l2.elem = 2;
    l3.elem = 3;

    l1.next = &l2;
    l2.next = &l3;
    l3.next = [^List]0;

    print_list(&l1);

    return 0;
}