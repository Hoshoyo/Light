string :: struct {
	length   : s64;
	capacity : s64;
	data     : ^u8;
}

memcpy :: (dest : ^void, src : ^void, size : u64) -> ^void {
	i :u64= 0;
	while i < size {
		*[^u8](dest + i) = *[^u8](src + i);
		i += 1;
	}
}
main :: () -> s32 {
	a := "Hello".data;
	return [s32]a;
}