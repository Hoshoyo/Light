#import "linux/print.li"

main :: () -> s32 {
	token := "1.1";
	result := 0.0;

	multiplier := 10.0;
	m := 1.0;
	for i := 0; i < token.length; i += 1 {
		if token.data[i] == '.' {
			multiplier = 0.1;
			m = 0.1;
			continue;
		}
		result += m * [r32](token.data[i] - 0x30);
		m *= multiplier;
	}
	print_r32(result);

	return 0;
}
