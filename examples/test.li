ExitProcess  :: (exit_code : s32) -> s32 #foreign("kernel32");
print_string :: (length : s64, data : ^s8) -> void #foreign("msvcrt");
//print_s64 :: (v : s64) -> s64 #foreign("msvcrt");
print_double :: (v : r64) -> void #foreign("print_string");

factorial :: (val : s32) -> s32 {
	if(val == 1) {
		return 1;
	}
	return val * factorial(val - 1);
}

fibonacci :: (val : s32) -> s32 {
	if(val == 0) {
		return 0;
	} else {
		if(val == 1) {
			return 1;
		} else {
			return fibonacci(val - 1) + fibonacci(val - 2);
		}
	}
	return 0;
}

main :: () -> s32 {
	f := fibonacci(4);
	ExitProcess(f);
	return 0;
}