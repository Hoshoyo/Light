#import "./examples/print.li"

main :: () -> s32 {
    // TODO: constants support for type declaration
    MAXY :: 4;
    MAXX :: 4;

    matrix : [4][4]r32;
    
    x := 0;
    y := 0;

    for y < MAXY {
        x = 0;
        for x < MAXX {
            matrix[y][x] = cast(r32)(y * x);
            x += 1;
        }
        y += 1;
    }

    print_s32(cast(s32)matrix[3][3]);

    return 0;
}