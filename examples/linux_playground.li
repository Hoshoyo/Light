#import "linux/print.li"

factorial :: (v : s32) -> s32 {
    if v == 1 return 1;
    return v * factorial(v - 1);
}

fill_array :: () {
    a : [3]s32;

    for i := 0; i < 3; i += 1 {
        a[i] = [s32]i;
    }

    for i := 0; i < 3; i += 1 {
        print_s64([s64]a[i]);
    }
}

main :: () -> s64 {
    fill_array();

    return 0;
}
