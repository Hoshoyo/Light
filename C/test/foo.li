#import "../modules/reflect.li"

string struct {
    capacity : u64;
    length   : u64;
    data     : ^u8;
}

array struct {
    capacity  : u64;
    length    : u64;
    //type_info : ^User_Type_Info;
    //type_info : ^void;
    data      : ^void;
}
hellow : (args : ...) -> s32 {
    a := args -> ^array;
    sum : s32 = 0;
    for i :u64= 0; i < a.length; i += 1 {
        d := (a.data -> ^s32)[i];
        sum = sum + d;
    }
    return sum;
}

main : () -> s32 {
