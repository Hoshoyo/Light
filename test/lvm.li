foo : () -> s32 {
    a : [4]s32;
    a[factorial(2)] = 3;
    return a[2];
    //return factorial(2);
}

factorial : (value : s32) -> s32 {
    b := 1;
    for a :s32= 1; a <= value ; a += 1 {
        b *= a;
    }
    return b;
}
