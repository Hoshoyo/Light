GetStdHandle : (std_handle : s32) -> ^void #extern("Kernel32.dll");
WriteConsoleA : (handle : ^void, str : ^u8, length : s32, written : ^s32, reserved : s32) -> bool #extern("Kernel32.dll");

print_sections:(s : string) -> s32 {
    stdout := GetStdHandle(-11);
    written : s32;
    WriteConsoleA(stdout, s.data, s.length -> s32, &written, 0);
    return 0;
}

main : () -> s32 {
    print_sections("1");
    print_sections("2");
    return 0;
}