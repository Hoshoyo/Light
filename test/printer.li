#import "../modules/reflect.li"
#import "../modules/w_print.li"

main : () -> s32 {    
    return print_test2(11, 22, 33);
}

print_test2:(args : ...) -> s32 {
    a := args -> ^array;
    values := a.data -> ^User_Type_Value;
    count := a.length;

    for i :u64= 0; i < count; i += 1 {
        value := values[i];
        v := *(value.value -> ^s32);
        print_s32(v);
        print_string("\n");
    }

    return count -> s32;
}


